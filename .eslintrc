{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [],
    "extends": ["eslint:recommended"],
    "rules": {
        "comma-dangle": 0,
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-unexpected-multiline": "warn",
        "valid-jsdoc" : [2]
    },
    "settings": {
    },

    "env": {
        "browser": true
    }
  }